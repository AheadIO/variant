#include <boost/variant2/variant.hpp>

template <int>
struct S {};

int main() {
  boost::variant2::variant<
      <%= (0...n).map { |i| "S<#{i}>" }.join(', ') %>> v(S<0>{});
  boost::variant2::variant<
      <%= (n - 1).downto(0).map { |i| "S<#{i}>" }.join(', ') %>> w(S<0>{});
#if defined(METABENCH)
  boost::variant2::visit([](auto, auto) {}, v, w);
  boost::variant2::visit([](auto, auto) {}, w, v);
#endif
}
