#include <functional>
#include <variant>

template <int>
struct S {};

int main() {
  std::experimental::variant<
      <%= (0...n).map { |i| "S<#{i}>" }.join(', ') %>> v(S<0>{});
  std::experimental::variant<
      <%= (n - 1).downto(0).map { |i| "S<#{i}>" }.join(', ') %>> w(S<0>{});
#if defined(METABENCH)
  std::experimental::visit([](auto, auto) {}, v, w);
  std::experimental::visit([](auto, auto) {}, w, v);
#endif
}
