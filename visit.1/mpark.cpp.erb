#include <mpark/variant.hpp>

template <int>
struct S {};

struct V {
  template <typename T>
  void operator()(T) const {}
};

int main() {
<%=
  (0...env[:repeat]).map { |i|
    alts = (0...n).map { |alt| "S<#{alt + i}>" }.join(", ")
    "  mpark::variant<#{alts}> v#{i}(S<#{i}>{});"
  }.join("\n")
%>
#if defined(METABENCH)
<%=
  (0...env[:repeat]).map { |i|
    "  mpark::visit(V{}, v#{i});"
  }.join("\n")
%>
#endif
}
