#include <boost/variant2/variant.hpp>

template <int>
struct S {};

int main() {
<%=
  (0...env[:repeat]).map { |i|
    alts = (0...n).map { |alt| "S<#{alt + i}>" }.join(", ")
    "  boost::variant2::variant<#{alts}> v#{i}(S<#{i}>{});"
  }.join("\n")
%>
#if defined(METABENCH)
<%=
  (0...env[:repeat]).map { |i|
    "  boost::variant2::visit([](auto) {}, v#{i});"
  }.join("\n")
%>
#endif
}
