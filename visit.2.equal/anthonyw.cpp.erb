#include <functional>
#include <variant>

template <int>
struct S {};

struct V {
  template <typename T, typename U>
  void operator()(T, U) const {}
};

int main() {
<%=
  (0...env[:repeat]).map { |i|
    alts = (0...n).map { |alt| "S<#{alt + i}>" }.join(", ")
    "  std::experimental::variant<#{alts}>\n" \
    "      v#{i}(S<#{i}>{}),\n"               \
    "      w#{i}(S<#{(n - 1) + i}>{});"
  }.join("\n")
%>
#if defined(METABENCH)
<%=
  (0...env[:repeat]).map { |i|
    "  std::experimental::visit(V{}, v#{i}, v#{i});\n" \
    "  std::experimental::visit(V{}, v#{i}, w#{i});\n" \
    "  std::experimental::visit(V{}, w#{i}, v#{i});\n" \
    "  std::experimental::visit(V{}, w#{i}, w#{i});"
  }.join("\n")
%>
#endif
}
